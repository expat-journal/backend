{
  "name": "expat-backend",
  "version": "1.0.0",
  "description": "Instagram for travelers. ",
  "main": "index.js",
  "scripts": {
    "rollback": "knex migrate:rollback --env production",
    "test": "cross-env DB_ENV=testing jest --watch --verbose",
    "start": "node index.js",
    "migrate": "knex migrate:latest --env production",
    "seed": "knex seed:run --env production",
    "api-docs": "apidoc -f .js -i ./routes -o apidoc/",
    "build-docs": "apidoc-markdown -p ./apidoc/ -o README.md",
    "build-api": "npm-run-all api-docs build-docs"
  },
  "author": "jeremiah tenbrink",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "csv-parser": "^2.2.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "fuse.js": "^3.4.4",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.5",
    "knex-cleaner": "^1.1.4",
    "minisearch": "^1.3.0",
    "moment": "^2.24.0",
    "pg": "^7.9.0",
    "promise": "^8.0.3",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2"
  }
}
